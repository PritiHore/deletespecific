#include<stdio.h>
int main(){
    int n,i,a[100],pos;
    printf("enter number of element");
    scanf("%d",&n);
    printf("enter elememt");
    for(i=0;i<n;i++)
    {

        scanf("%d",&a[i]);
    }
    printf("enter the position");
    scanf("%d",&pos);
    for(i=pos;i<n;i++){
    a[i]=a[i+1];
    }
   for(i=0;i<n-1;i++)
    {
        printf("%d",a[i]);
    }
    return 0;
}
